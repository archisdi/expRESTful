{
  "name": "express-boilerplate-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www",
    "server": "node ./bin/www",
    "clean-install": "rm -rf node_modules package-lock.json && npm install --save",
    "migrate": "cd ./database && sequelize db:migrate",
    "migrate:rollback": "cd ./database && sequelize db:migrate:undo:all",
    "migrate:refresh": "npm run migrate:rollback && npm run migrate"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.18.2",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "~4.15.5",
    "express-rate-limit": "^2.11.0",
    "helmet": "^3.12.0",
    "joi": "^13.1.2",
    "jsonwebtoken": "^8.2.0",
    "morgan": "~1.9.0",
    "mysql2": "^1.5.2",
    "sequelize": "^4.35.2"
  },
  "devDependencies": {
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "nodemon": "^1.17.1"
  }
}
